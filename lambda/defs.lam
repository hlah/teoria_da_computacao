let
-- operações lógicas --
true          = (\a.\b. a);
false         = (\a.\b. b);
if            = (\c.\a.\b. c a b);
not      = (\a.a false true);
and      = (\a.\b. a b false);
or       = (\a.\b. a true b);

-- testes --
zero    = (\a.a (\x.false) true); 

-- operações aritiméticas --
sucessor      = (\n.\f.\x.f (n f x));
soma          = \a.\b.\x.\y.(a x)((b x) y);
multiplica    = \a.\b.\x. a (b x);

in
soma 100 100
